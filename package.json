{
  "name": "dancarr.io",
  "version": "1.0.0",
  "description": "2017 version of the Dan Carr website.",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=hotreloading node app.js",
    "start:dev": "npm run dev && NODE_ENV=development node app.js",
    "start:prod": "npm run build && NODE_ENV=production node app.js",
    "dev": "webpack --config ./webpack/config.dev.js --progress --colors",
    "build": "webpack --config ./webpack/config.prod.js --progress --colors",
    "test": "tape -r ./test/_helpers/jsdom.js ./test/**/*.js | faucet",
    "watch": "tape-watch ./test/**/*.js | faucet"
  },
  "keywords": [
    "AWS",
    "Node",
    "Express",
    "React",
    "Redux",
    "Components",
    "SPA",
    "Personal Portfolio"
  ],
  "dependencies": {
    "body-parser": "^1.16.1",
    "express": "^4.14.1",
    "morgan": "^1.8.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "serve-favicon": "^2.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-airbnb": "^1.1.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.0",
    "css-loader": "^0.26.1",
    "ejs": "^2.5.6",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "faucet": "0.0.1",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^7.2.1",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.13.1",
    "tape": "^4.6.3",
    "tape-plus": "^1.0.0",
    "tape-watch": "^2.2.4",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.17.0"
  },
  "author": "Dan Carr",
  "license": "ISC"
}
